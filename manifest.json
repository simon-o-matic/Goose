{
  "name": "Google Shortcuts Extension (Goose)",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Keyboard shortcuts for Google Search.",
  "icons": {"48": "images/icon40.png"},
  "content_scripts": [
    {
      "matches": [
        "*://www.google.com/*",
        "*://www.google.com.au/*"
      ],
      "css": ["goosestyle.css"],
      "js": [
        "jquery-1.8.3.min.js", 
        "goose.js"
      ]
    }
  ]
}
